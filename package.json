{
  "name": "easy-apps",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "json-server": "^0.16.3",
    "node-sass": "^5.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "json-server": "json-server --port 3001 --watch src/db/wordList.json",
    "scss-calc": "node-sass src/Apps/Calculator/style/App.scss src/Apps/Calculator/style/App.css --output-style compressed --watch &",
    "scss-calc-button": "node-sass src/Apps/Calculator/style/Button.scss src/Apps/Calculator/style/Button.css --output-style compressed --watch &",
    "scss-typingGame": "node-sass src/Apps/TypingGame/style/TypingGame.scss src/Apps/TypingGame/style/TypingGame.css --output-style compressed --watch &",
    "scss-typingGame-word": "node-sass src/Apps/TypingGame/style/Word.scss src/Apps/TypingGame/style/Word.css --output-style compressed --watch &",
    "scss-typingGame-char": "node-sass src/Apps/TypingGame/style/Char.scss src/Apps/TypingGame/style/Char.css --output-style compressed --watch &"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
